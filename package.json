{
  "name": "vue-message-viewer",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/vue-message-viewer.umd.cjs",
  "module": "dist/vue-message-viewer.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-message-viewer.js",
