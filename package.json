    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vue-tsc": "^1.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pmanikas/vue-message-viewer"
  },
  "keywords": [
    "vue",
    "message",
    "viewer"
  ],
